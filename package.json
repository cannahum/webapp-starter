{
  "name": "webapp-starter",
  "version": "1.0.0",
  "description": "A Full Stack Web Application Starter with Node, Express, Webpack, React, Scss and TypeScript.",
  "main": "src/index.ts",
  "repository": "https://github.com/cannahum/webapp-starter.git",
  "author": "cannahum@outlook.com",
  "license": "MIT",
  "scripts": {
    "start": "npm start --prefix src/server",
    "start:dev": "npm run --prefix src/server start:dev",
    "test": "export NODE_ENV=test && jest",
    "build": "npm run --prefix src/client build",
    "build:watch": "webpack-dev-server --config config/webpack.dev-server.js",
    "build:prod": "webpack -p --config config/webpack.prod.js"
  }
}
